generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id          Int                          @id @default(autoincrement())
  title       String
  description String
  releaseYear Int
  genre       String
  createdAt   DateTime                     @default(now())
  updatedAt   DateTime                     @updatedAt
  vector      Unsupported("vector(1536)")?
}
